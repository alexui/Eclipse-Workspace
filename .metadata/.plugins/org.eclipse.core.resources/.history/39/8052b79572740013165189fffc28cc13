package Ex1;

public class Animal {

	private String type;
	private Integer cageNumber;
	
	public Animal(String type, Integer cageNumber) {
		super();
		this.type = type;
		this.cageNumber = cageNumber;
	}

	public String getType() {
		return type;
	}

	public void setType(String type) {
		this.type = type;
	}

	public Integer getCageNumber() {
		return cageNumber;
	}

	public void setCageNumber(Integer cageNumber) {
		this.cageNumber = cageNumber;
	}
	
	@Override
	public String toString(){
		return ""+this.type+" "+this.cageNumber;
	}
	
	@Override
	public boolean equals(Animal a){
		
		(a.type==type && a.cageNumber == cageNumber)? return true : return false;
	}
}
