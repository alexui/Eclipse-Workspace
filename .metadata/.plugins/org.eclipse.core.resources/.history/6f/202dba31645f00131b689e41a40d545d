
public class Student {

	private String nume;
	private int varsta;
	
	
	
	public Student(String nume, int varsta) {
		super();
		this.nume = nume;
		this.varsta = varsta;
	}

	public String getNume() {
		return nume;
	}

	public void setNume(String nume) {
		this.nume = nume;
	}

	public int getVarsta() {
		return varsta;
	}

	public void setVarsta(int varsta) {
		this.varsta = varsta;
	}

	@Override
	public int hashCode()
	{
		String s = String.valueOf(this.nume.charAt(0)).toLowerCase();
		return s.hashCode()+this.varsta; 
	}
	
	public boolean equals(Object o)
	{
		Student s = (Student)o;
		if()
	}
}
